{
    "http": {
        "port": 3000
    },
    "elastic": {
        "index": "copernicus",
        "type": "resource",
        "enableValuesCounting": true
    },
    "external_templates": {
        "local_path": "/external_templates",
        "protocol": "http",
        "host_cop": "copernicus.europa.eu",
        "head_path_cop": "/copernicus/external-template-head",
        "header_path_cop": "/copernicus/external-template-header",
        "footer_path_cop": "/copernicus/external-template-footer",

        "host": "www.eea.europa.eu",
        "head_path": "/templates/v2/getRequiredHead?jsdisable=all&render_full_html=False&cssonly=True",
        "header_path": "/templates/v2/getHeader?render_full_html=False",
        "footer_path": "/templates/v2/getFooter?render_full_html=False"
    },
    "layout_vars": {
        "head_title": "Global catalogue of the European Environment Agency (EEA)",
        "app_id": "copernicus-search",
        "skip_resources_bundling": true,
        "skip_external_template_styles": true,
        "override_searchserver_resources": false,
        "css_resources": {
            "index_page": [
                "/custom.css",
                "css/esbootstrap.facetview.css"
            ]
        },
        "js_resources": {
            "index_page": [
                "/custom.js"
            ]
        },
        "site_title": "Copernicus search",
        "enableBreadcrumbs": true,
        "breadcrumbs": "Copernicus Search",
        "dataprovencance_info_text": "Semantic Data Service (SDS)",
        "dataprovencance_info_url": "http://semantic.eea.europa.eu",
        "further_info": ""
    },
    "es_debug_enabled": false,
    "suggestions_enabled": true,
    "relevance_enabled": true,
    "river_configs": {
        "configs": [
            {
                "id": "eea_copernicus",
                "cluster_name": "Copernicus Land at EEA (land.copernicus.eu)",
                "config_file": "riverconfig_land.copernicus_data.json"
            }
        ]
    },
    "endpoint": "http://semantic.eea.europa.eu/sparql"
}
